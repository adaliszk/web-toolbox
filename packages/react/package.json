{
    "version": "18.3.11",
    "name": "@adaliszk/react",
    "description": "React stack and configuration for Development using Typescript",
    "repository": "github:adaliszk/node-toolbox",
    "homepage": "https://github.com/adaliszk/node-toolbox/tree/release/packages/react",
    "bugs": "https://github.com/adaliszk/node-toolbox/labels/react",
    "license": "BSD-3-Clause",
    "types": "./dist/bundle.d.ts",
    "module": "./dist/bundle.mjs",
    "main": "./dist/bundle.cjs",
    "files": [
        "dist",
        "*.json",
        "*.md"
    ],
    "scripts": {
        "build": "tsup",
        "lint": "biome lint --apply ./source",
        "fmt": "biome format -w .",
        "prepublish": "pnpm build"
    },
    "peerDependencies": {
        "typescript": "^5.4"
    },
    "dependencies": {
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router": "^6.23.0",
        "react-router-dom": "^6.23.0"
    },
    "devDependencies": {
        "@adaliszk/typescript": "workspace:*"
    },
    "keywords": [
        "typescript",
        "react",
        "framework",
        "bundle"
    ],
    "exports": {
        ".": {
            "require": "./tsconfig.json",
            "import": "./dist/bundle.mjs",
            "types": "./dist/bundle.d.ts"
        },
        "./config": {
            "require": "./tsconfig.json",
            "import": "./tsconfig.json"
        },
        "./server": {
            "require": "./dist/server.cjs",
            "import": "./dist/server.mjs",
            "types": "./dist/server.d.ts"
        },
        "./client": {
            "require": "./dist/client.cjs",
            "import": "./dist/client.mjs",
            "types": "./dist/client.d.ts"
        }
    }
}
