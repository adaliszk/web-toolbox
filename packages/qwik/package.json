{
    "version": "1.5.7",
    "name": "@adaliszk/qwik",
    "description": "Qwik stack and configuration for Development using Typescript",
    "repository": "github:adaliszk/node-toolbox",
    "homepage": "https://github.com/adaliszk/node-toolbox/tree/release/packages/qwik",
    "bugs": "https://github.com/adaliszk/node-toolbox/labels/qwik",
    "license": "BSD-2-Clause",
    "types": "dist/bundle.d.ts",
    "module": "dist/bundle.mjs",
    "main": "dist/bundle.js",
    "scripts": {
        "build": "pnpm build.bundle && pnpm build.config",
        "build.bundle": "tsup source/bundle.mts --format cjs,esm --minify --sourcemap --dts --out-dir dist",
        "build.config": "cp tsconfig.json config",
        "lint": "biome lint --apply ./source",
        "fmt": "biome format -w ."
    },
    "files": ["dist", "config", "*.json", "*.md"],
    "peerDependencies": {
        "@builder.io/qwik": "^1.5",
        "@builder.io/qwik-city": "^1.5",
        "typescript": "^5.4",
        "undici": "^6.15"
    },
    "dependencies": {
        "@types/node": "^22.5.2"
    },
    "devDependencies": {
        "@adaliszk/typescript": "workspace:*",
        "@builder.io/qwik": "^1.5.3",
        "@builder.io/qwik-city": "^1.5.3",
        "undici": "^6.15.0"
    },
    "keywords": [
        "typescript",
        "qwik",
        "resumable react",
        "static-content generator",
        "server-side renderer",
        "react-like"
    ],
    "exports": {
        ".": {
            "require": "./tsconfig.json",
            "import": "./dist/bundle.mjs"
        },
        "./config": {
            "require": "./tsconfig.json",
            "import": "./tsconfig.json"
        }
    }
}
